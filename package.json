{
  "name": "timetracker-daily-import",
  "displayName": "timetracker-daily-import",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.109.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:markdown",
    "onCommand:timetracker-daily-import.helloWorld",
    "onCommand:timetracker.loadWorkItems",
    "onCommand:timetracker.insertWorkItem"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "timetracker-daily-import.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "timetracker.loadWorkItems",
       "title": "TimeTracker: Load Work Items (Debug)"
      },
      {
        "command": "timetracker.insertWorkItem",
       "title": "TimeTracker: Insert Work Item"
      }
    ],
    "configuration":{
      "title": "TimeTracker Daily Import",
      "properties": {
         "timetrackerDailyImport.workItemsPath": {
           "type": "string",
           "default": "daily/work_items.json",
           "description": "Path to work_items.json relative to the workspace root."
         }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.109.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.54.0",
    "eslint": "^9.39.2",
    "esbuild": "^0.27.2",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}
